docker exec -u postgres bazaSpremnik psql -d ORmuzeji -c "COPY (SELECT to_jsonb(array_agg(muzeji)) FROM (SELECT m.naziv, drzava, grad, godina_osnivanja, m.tip, velicina_kolekcije, posjetitelji, izlozbeni_prostor, web_stranica, online_setnja, to_jsonb(array_agg(json_build_object('naziv', e.naziv, 'tip', e.tip))) AS eksponati FROM muzeji m JOIN eksponati e ON m.id = e.id_muzej GROUP BY m.id) AS muzeji) TO STDOUT" > novi_json.json