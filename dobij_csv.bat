docker exec -u postgres bazaSpremnik psql -d ORmuzeji -c "COPY (SELECT muzeji.naziv, drzava, grad, godina_osnivanja, muzeji.tip, velicina_kolekcije, posjetitelji, izlozbeni_prostor, web_stranica, online_setnja, eksponati.naziv, eksponati.tip FROM muzeji JOIN eksponati ON muzeji.id = eksponati.id_muzej ORDER BY muzeji.id, eksponati.id) TO STDOUT WITH (FORMAT CSV, HEADER)" > novi_csv.csv